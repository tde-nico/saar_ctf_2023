[Unit]
Description=telework-holiday
After=network.target

[Service]
Type=simple
ExecStart=/home/telework/holiday/Holiday/bin/Release/net7.0/Holiday
WorkingDirectory=/home/telework/holiday/Holiday/bin/Release/net7.0
User=telework
Group=telework
Environment=PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin

[Install]
WantedBy=multi-user.target
